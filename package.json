{
  "name": "ecommerce",
  "version": "0.1.1",
  "description": "",
  "keywords": [],
  "author": "Hashem Al-Radaideh",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently 'next dev' 'nodemon' 'prisma generate --watch' 'prisma studio --browser none' ",
    "start": "concurrently 'bun server:start' 'bun client:build && bun client:start'",
    "server:dev": "nodemon",
    "server:start": "NODE_ENV=production bun api/index.ts",
    "next": "next",
    "client:dev": "next dev",
    "client:build": "prisma generate && NODE_ENV=production next build",
    "client:start": "NODE_ENV=production next start",
    "db:mysql": "docker run --name mysqldb -p 3306:3306 -e MYSQL_ROOT_PASSWORD=root -d mysql",
    "db:postgres": "docker run --name postgresdb -p 5432:5432 -e POSTGRES_PASSWORD=postgres -d postgres",
    "cache": "docker run --name rediscache -d -p 6379:6379 redis --requirepass foobared",
    "prisma": "prisma",
    "studio": "prisma studio",
    "migrate": "prisma migrate dev",
    "pull": "prisma db pull",
    "push": "prisma db push",
    "generate": "prisma generate",
    "tauri": "tauri",
    "prettier": "prettier . --write --log-level warn",
    "lint": "next lint",
    "check": "bun prettier && bun lint"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.14.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@react-google-maps/api": "^2.19.3",
    "@tanstack/react-query": "^5.35.1",
    "@tanstack/react-table": "^8.16.0",
    "@tauri-apps/api": "^1",
    "@types/uuid": "^9.0.8",
    "@upstash/redis": "^1.31.2",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "embla-carousel-autoplay": "^8.0.4",
    "embla-carousel-react": "^8.0.4",
    "express": "^4.19.2",
    "jose": "^5.3.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.378.0",
    "morgan": "^1.10.0",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.4",
    "sharp": "^0.33.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.12",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/mysql": "^2.15.26",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
